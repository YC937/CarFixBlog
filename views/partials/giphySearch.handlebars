  <section>
  <form id="search-form">
    <div class="input-group">
        <div class="form-outline">
            <label class="form-label" for="search"> Type in your favorite word! </label>
            <input type="text" id="search" class="form-control" />
        </div>
        <button type="button" class="btn btn primary">
            <i class="fas fa-search"></i>
        </button>
    </div>
  </form>
  <ul>
    {{#each giphyData}}
    <li>
        <img src="{{url}}" alt="{{alt}}" />
    </li>
    {{/each}}
  </ul>

  </section>

  <script src="/js/search-giphy.js"></script>

  {{!-- Fix styling, center search bar and giphy images --}}